{
    "manifest_version": 2,
    "name": "Password Generator",
    "version": "1.0",
    "description": "Generate strong passwords and store them securely.",
    "permissions": ["storage"],
    "browser_action": {
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      },
      "default_title": "Password Generator",
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  